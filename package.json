{
  "name": "procurement-backend",
  "version": "1.0.0",
  "description": "This is a backend application for ab-inbev procurement project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "compile": "babel src --out-dir .compiled --source-maps --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed:reset": "sequelize db:seed:undo:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "prestart": "npm run migrate",
    "start:dev": "npm run migrate && nodemon --exec babel-node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CommercialTech-ABInBev/Procurement-Backend.git"
  },
  "keywords": [
    "procurement",
    "Backend",
    "ab-inbev"
  ],
  "author": "Tochukwu Ozurumba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CommercialTech-ABInBev/Procurement-Backend/issues"
  },
  "homepage": "https://github.com/CommercialTech-ABInBev/Procurement-Backend#readme",
  "dependencies": {
    "@azure/storage-blob": "^12.3.0",
    "@babel/polyfill": "^7.12.1",
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^7.4.0",
    "@types/hapi__joi": "^15.0.3",
    "azure-storage": "^2.10.3",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "cloudinary": "^1.23.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "errorhandler": "^1.5.1",
    "exceljs": "^4.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "joi-password-complexity": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "^1.1.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "paystack": "^2.0.1",
    "paystack-node": "^0.2.5",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^5.15.0",
    "sequelize-cli": "^5.5.0",
    "sharp": "^0.26.3",
    "underscore": "^1.12.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0"
  }
}
